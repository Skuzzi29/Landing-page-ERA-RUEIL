{
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "estimation-rueil-malmaison.fr" }],
      "destination": "https://www.estimation-rueil-malmaison.fr/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/sitemap.xml",
      "headers": [{ "key": "Content-Type", "value": "application/xml; charset=utf-8" }]
    },
    {
      "source": "/robots.txt",
      "headers": [{ "key": "Content-Type", "value": "text/plain; charset=utf-8" }]
    }
  ]
}
